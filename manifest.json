{
	"name": "Portland State RateMyProfessors",
	"description": "Displays professors' RateMyProfessors ratings in the online course listings for Portland State University.",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"permissions": [
		"*://*.ratemyprofessors.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://app.banner.pdx.edu/*"
			],
			"exclude_matches": [
				"https://app.banner.pdx.edu/mobileAppPromo.png"
			],
			"css": [
				"dist/css/tooltipster.bundle.css"
			],
			"js": [
				"jquery.js",
				"dist/js/tooltipster.bundle.js",
				"rmp.js"
			]
		}
	],
	"background": {
		"scripts": [
			"jquery.js",
			"background.js"
		],
		"persistent": false
	}
}